#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main()
{
   int n,m,i,j;
   cin>>n>>m;
   vector<int>arr(n);
   for(i=0;i<n;i++){
       cin>>arr[i];
   }
    vector<int>arr1(m);
    for(j=0;j<m;j++){
        cin>>arr1[j];
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
        if(arr[i]==arr1[j]){
            cout<<arr1[j];
        }
        }
    }
    return 0;
}
